    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "vue-template-compiler": ">= 2.6.14 < 2.7"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "uni-app": {
    "scripts": {}
  },
  "packageManager": "yarn@4.0.0"
}

    "@dcloudio/uni-mp-xhs": "2.0.2-3090420231025001",
    "@dcloudio/uni-quickapp-native": "2.0.2-3090420231025001",
    "@dcloudio/uni-quickapp-webview": "2.0.2-3090420231025001",
    "@dcloudio/uni-stacktracey": "2.0.2-3090420231025001",
    "@dcloudio/uni-stat": "2.0.2-3090420231025001",
    "@metamask/detect-provider": "^2.0.0",
